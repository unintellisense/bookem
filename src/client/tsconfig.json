{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "jsx": "react",
    "sourceMap": true,
    "noLib": false
